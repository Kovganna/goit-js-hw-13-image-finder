{{#each this}}

<div class="photo-card">
  <img src="{{webformatURL}}" alt="{{tags}}" width="400" height="200"/>

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{visibility}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{downloads}}
    </p>
  </div>
</div>

{{/each}}
